<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd" id="mascotee" metadata-complete="false" version="3.0">
  <enterprise-beans>
    <session>
      <ejb-name>DatFileService</ejb-name>
      <service-endpoint>au.edu.unimelb.plantcell.servers.mascotee.endpoints.DatFileService</service-endpoint>
      <ejb-class>au.edu.unimelb.plantcell.servers.mascotee.impl.DatFileServiceImpl</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
    </session>
    
    <session>
      <ejb-name>ConfigService</ejb-name>
      <service-endpoint>au.edu.unimelb.plantcell.servers.mascotee.endpoints.ConfigService</service-endpoint>
      <ejb-class>au.edu.unimelb.plantcell.servers.mascotee.impl.ConfigServiceImpl</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
    </session>
    
    <session>
      <ejb-name>SearchService</ejb-name>
      <service-endpoint>au.edu.unimelb.plantcell.servers.mascotee.endpoints.SearchService</service-endpoint>
      <ejb-class>au.edu.unimelb.plantcell.servers.mascotee.impl.SearchServiceImpl</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
    </session>
 
    <session>
      <ejb-name>MascotConfig</ejb-name>
      <ejb-class>au.edu.unimelb.plantcell.servers.mascot.core.v2.MascotConfig</ejb-class>
      <session-type>Stateful</session-type>
    </session>
  </enterprise-beans>
</ejb-jar>